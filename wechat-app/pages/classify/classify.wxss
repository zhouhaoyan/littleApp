.classify_view{
  display: flex;
  flex: row;
  flex-wrap: wrap;
}


.classify_image_view {
  width: 50%;
    align-items: center;
    justify-content:center;
    display:flex;/*Flex布局*/
}

.classify_image {
  width: 100%;
